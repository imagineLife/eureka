{
  "name": "eureka-server",
  "version": "0.0.1",
  "private": true,
  "proxy": "http://localhost:3001/",
  "main": "server.js",
  "dependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "6.14.0",
    "babel-preset-react": "^6.23.0",
    "classnames": "^2.2.5",
    "express": "4.13.3",
    "express-formidable": "^1.0.0",
    "filesize": "^3.5.6",
    "fs": "0.0.2",
    "html-entities": "^1.2.0",
    "path-parse": "^1.0.5",
    "react": "^15.4.2",
    "react-addons-update": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-dropzone": "^3.12.2",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0"
  },
  "engines": {
    "node": "6.5.0"
  },
  "scripts": {
    "start": "yarn build && yarn prod",
    "prestart": "yarn && yarn fetchsourcesprod && yarn fetchsources",
    "fetchsources": "rm -rf sources.zip && rm -rf sources && wget http://j4p.us/0f2O0S1Q362K/sources.zip && unzip sources.zip && rm -rf sources.zip && ls && ls sources",
    "fetchsourcesprod": "cd ./prod && rm -rf sources.zip && rm -rf sources && wget http://j4p.us/0f2O0S1Q362K/sources.zip && unzip sources.zip && rm -rf sources.zip && ls && ls sources && cd ../",
    "buildserver": "babel ./server.js -d prod",
    "build": "yarn buildserver && babel ./client/src -d prod/client/src && cp -R client/build prod/client",
    "serve": "babel-node server.js",
    "server": "yarn serve",
    "prod": "NODE_ENV=production node prod/server.js",
    "client": "babel-node start-client.js",
    "herokuforce": "git push heroku release-2.0:master --force",
    "dev": "echo \"This command has been deprecated. Use 'npm start'\" && exit 1",
    "lint": "eslint ."
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.0",
    "concurrently": "3.1.0",
    "eslint": "3.15.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.9.0",
    "formidable": "^1.1.1",
    "rmdir": "^1.2.0"
  }
}
